{
    "name": "soapbox/serialized-payloads",
    "type": "library",
    "description": "A Laravel library that allows you to serialize payloads for things like queue jobs.",
    "license": "MIT",
    "require": {
        "ramsey/uuid": "^4.0",
        "illuminate/support": "^8.0",
        "illuminate/database": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "SoapBox\\SerializedPayloads\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "SoapBox\\SerializedPayloads\\Tests\\": "tests/",
            "SoapBox\\SerializedPayloads\\Factories\\": "factories/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "orchestra/testbench": "^6.0",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SoapBox\\SerializedPayloads\\ServiceProvider"
            ]
        }
    }
}
